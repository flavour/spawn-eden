<uwsgi>
    <uid>web2py</uid>
    <gid>web2py</gid>
    <pythonpath>/home/web2py/</pythonpath>
    <app mountpoint="/">
        <script>wsgihandler</script>
    </app>
    <mule>run_scheduler.py</mule>
    <workers>4</workers>
    <pidfile>/tmp/uwsgi-prod.pid</pidfile>
    <daemonize>/var/log/uwsgi/prod.log</daemonize>
    <socket>127.0.0.1:59025</socket>
    <master/>
</uwsgi>
